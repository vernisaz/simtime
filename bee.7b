# simtime build script

crate  =simtime
crate main=lib
crate_src=.
dep_crates=[]
comp opts=[]
test file=test
common =..${~/~}simscript${~/~}comm-crate.7b:file
common_test =..${~/~}simscript${~/~}comm-test.7b:file
crate_dir=..${~/~}crates

version=1.04b14
set_env(VERSION,version)
# probably move it in common scripts
mode=mode:prop
if {
	eq(mode,release) then {
		array(-C,opt-level=3)
		assign(crate opts, ~~)
	}
}

include(common);
include(common_test)